{
  "name": "@tinybirdco/sdk",
  "version": "0.0.9",
  "description": "TypeScript SDK for Tinybird Forward - define datasources and pipes as TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tinybird": "./bin/tinybird.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "tinybird",
    "analytics",
    "typescript",
    "sdk",
    "clickhouse",
    "cli"
  ],
  "author": "Tinybird",
  "license": "MIT",
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "chokidar": "^4.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.24.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "msw": "^2.12.7",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.28.2"
}
